@model ECJ.Web.Models.tblSoumission
<h2>Détail de la soumission</h2>

<h2>Détail d'une soumission</h2>

<form name="createPub" role="form" novalidate class="form-validation">

    <div class="form-group">
        <label>Numéro de la Soumission</label>
        <div class="well well-sm">
            @Html.DisplayFor(model=> model.noSoumission)
        </div>
    </div>

    <div class="form-group">
        <label>Numéro de l'agence</label>
        <div class="well well-sm">
            @Html.DisplayFor(model => model.noSoumissionAgence)
        </div>
    </div>

    <div class="form-group">
        <label>Nom de la soumission</label>
        <div class="well well-sm">
            @Html.DisplayFor(model => model.nom)
        </div>
    </div>

    <div class="form-group">
        <label>Prix</label>
        <div class="well well-sm">
            @Html.DisplayFor(model => model.prix)
        </div>
    </div>
    <div class="form-group">
        <label>Commentaire</label>
        <div class="well well-sm">
            @Html.DisplayFor(model => model.commentaire)
        </div>
    </div>

    <div class="form-group">
        <label>Statut</label>
        <div class="well well-sm">
            @Html.DisplayFor(model => model.tblStatutAppelOffre.nom)
        </div>
    </div>

    <div class="form-group">
        <label>Statut de l'agence</label>
        <div class="well well-sm">
            @Html.DisplayFor(model => model.statut)
        </div>
    </div>
    <br />    
    <br />
    <a href="/AppellOffre/Index" class="btn btn-default pull-right " data-dismiss="modal">Retour</a>

</form>