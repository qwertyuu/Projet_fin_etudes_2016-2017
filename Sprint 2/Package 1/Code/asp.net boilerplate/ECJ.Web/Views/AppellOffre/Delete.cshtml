@model ECJ.Web.Models.tblAppelOffre

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Supprimer</title>
</head>
<body>
    <h3>Êtes vous sure de vouloir supprimer cet d'appel d'offre?</h3>
    <div>
   <form name="createPub" role="form" novalidate class="form-validation">

            <div class="form-group">
                <label>Évènement</label>
                <div class="well well-sm">
                    @Html.DisplayFor(model => model.tblEvenement.nom)
                </div>
            </div>

            <div class="form-group">
                <label>Nom</label>
                <div class="well well-sm">
                    @Html.DisplayFor(model => model.nom)
                </div>
            </div>

            <div class="form-group">
                <label>Média</label>
                <div class="well well-sm">
                    Radio
                </div>
            </div>

            <div class="form-group">
                <label>Date d'envoi</label>
                <div class="well well-sm">
                    @Html.DisplayFor(model => model.dateEnvoi)
                </div>
            </div>
            <div class="form-group">
                <label>Date requise</label>
                <div class="well well-sm">
                    @Html.DisplayFor(model => model.dateRequis)
                </div>
            </div>

            <div class="form-group">
                <label>Statut</label>
                <div class="well well-sm">
                    @Html.DisplayFor(model => model.tblStatut.nom)
                </div>
            </div>

            <div class="form-group">
                <label>Agence de publicité</label>
                <div class="well well-sm">
                    Médiative
                </div>
            </div>

            <div class="form-group">
                <label>Description</label>
                <div class="well well-sm">
                    @Html.DisplayFor(model => model.description)
                </div>
            </div>
</form>
            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()
                <br /><br /><br />
                <div class="form-actions no-color pull-right">
                    <button class="btn btn-primary blue" type="submit"><i class="fa fa-trash"> </i> Supprimer</button>
                    <a href="/AppellOffre/Index" class="btn btn-default" data-dismiss="modal">Annuler</a>
                </div>
            }
</div>
</body>
</html>
